    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if n <= 6:\n            for i in range(10**(n)-1, -1, -1):\n                if i%k == 0:\n                    s = str(i)\n                    if s == s[::-1]:\n                        return s\n        if k == 1 or k == 3 or k == 9:\n            return \"9\"*n\n        if k == 2:\n            return \"8\" + \"9\"*(n-2)+\"8\"\n        if k == 4:\n            return \"88\" + \"9\"*(n-4)+\"88\"\n        if k == 5:\n            return \"5\" + \"9\"*(n-2)+\"5\"\n        if k == 6:\n            if n%2 == 0:\n                cen = \"9\"*(n//2-3) + \"77\" + \"9\"*(n//2-3)\n            else:\n                cen = \"9\"*(n//2-2) + \"8\" + \"9\"*(n//2-2)\n            return \"89\" + cen +\"98\"\n        if k == 7:\n            if n%12 <= 6:\n                x = \"7\"\n                xx = \"5\"\n            else:\n                x = \"4\"\n                xx = \"6\"\n            if n % 6 == 0:\n                return \"9\"*n\n            if n % 6 == 1 or n % 6 == 5:\n                return \"9\"*(n//2) + x + \"9\"*(n//2)\n            if n % 6 == 2 or n % 6 == 4:\n                return \"9\"*(n//2-1) + x*2 + \"9\"*(n//2-1)\n            if n % 6 == 3:\n                return \"9\"*(n//2) + xx + \"9\"*(n//2)\n        if k == 8:\n            return \"888\" + \"9\"*(n-6)+\"888\"",
