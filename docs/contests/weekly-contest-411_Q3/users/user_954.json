    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        '''\n        bounds = (10**(n-1), 10**n)\n        for number in reversed(range(bounds[0], bounds[1])):\n            if str(number) == str(number)[::-1]:\n                if number % k == 0:\n                    return str(number)\n        '''\n        import sys\n        sys.set_int_max_str_digits(0)\n        if k == 1:\n            return str(10**n-1)\n        if k == 2:\n            if n <= 2:\n                return \"8\" * n\n            return \"8\" + \"9\" * (n-2) + \"8\"\n        if k == 3:\n            return str(10**n-1)\n        if k == 4:\n            if n <= 4:\n                return \"8\" * n\n            return \"8\"*2 + \"9\" * (n-4) + \"8\"*2\n        if k == 5:\n            if n <= 2:\n                return \"5\" * n\n            return \"5\" + (\"9\" * (n-2)) + \"5\"\n        if k == 6:\n            if n <= 2:\n                return \"6\" * n\n            if n == 3:\n                return \"888\"\n            if n == 4:\n                return \"8778\"\n            if n % 2 != 0:\n                return \"8\" + \"9\" * ((n-3)//2) + \"8\" + \"9\" * ((n-3)//2) + \"8\"\n            else:\n                return \"8\" + \"9\" * ((n-4)//2) + \"77\" + \"9\" * ((n-4)//2) + \"8\"\n        if k == 7:\n            if n <= 2:\n                return \"7\" * n\n            if n == 3:\n                return \"959\"\n            if n == 4:\n                return \"9779\"\n            if n == 5:\n                return \"99799\"\n            if n % 6 == 0:\n                return \"9\" * n\n            if (n-1) % 6 == 0:\n                if (n-1) % 12 == 0:\n                    return \"9\" * ((n-1)//2) + \"7\" + \"9\" * ((n-1)//2)\n                return \"9\" * ((n-1)//2) + \"4\" + \"9\" * ((n-1)//2)\n            if (n-3) % 6 == 0:\n                if (n-3) % 12 == 0:\n                    return \"9\" * ((n-1)//2) + \"5\" + \"9\" * ((n-1)//2)\n                return \"9\" * ((n-1)//2) + \"6\" + \"9\" * ((n-1)//2)\n            if ((n-2) % 6 == 0) or ((n-4) % 6 == 0):\n                if (n-2) % 12 == 0 or (n-4) % 12 == 0:\n                    return \"9\" * ((n-2)//2) + \"77\" + \"9\" * ((n-2)//2)\n                return \"9\" * ((n-2)//2) + \"44\" + \"9\" * ((n-2)//2)\n            if (n-5) % 6 == 0:\n                if (n-5) % 12 == 0:\n                    return \"9\" * ((n-1)//2) + \"7\" + \"9\" * ((n-1)//2)\n                return \"9\" * ((n-1)//2) + \"4\" + \"9\" * ((n-1)//2)\n        if k == 8:\n            if n <= 6:\n                return \"8\" * n\n            return \"8\" * 3 + \"9\" * (n-6) + \"8\" * 3\n        if k == 9:\n            return '9' * n\n\n        return 0\n\n",
