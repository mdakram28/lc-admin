    "submission": "class Solution {\nprivate:\n    vector<int> d9 = {0, 2, 1, 5, 3, 4}; // 6\n    vector<int> d0 = {1, 3, 2, 6, 4, 5}; // 6\n    string f(int len, int d) {\n        // cout << len << \" \" << d << endl;\n        int res = (d9[d % 6] + d9[d % 6] * d0[(d + len) % 6]) % 7;\n        // cout << d9[d % 6] << \" \"<< d9[d % 6] << \" \"<<  d0[(d + len) % 6] << endl;\n        int dh = d0[d % 6];\n        int h = (len == 1) ? 1 : 11;\n        for (int i = 9 * h; i >= 0; i -= h) {\n            if ((i * dh + res) % 7 == 0) return to_string(i);\n        }\n        return \"**\";\n    }\npublic:\n    // done 1, 2, 3, 4, 5, 6, 8, 9\n    // pending  7\n    \n    // 3, 6, 2, 5, 1, 4, 0, 3, 6\n    \n    string largestPalindrome(int n, int k) {\n        // cout << 9 % 7 << \" \" << 99 % 7 << \" \"<< 999 % 7 << \" \"<< 9999 % 7 << \" \"<< 99999 % 7 << \" \"<< 999999 % 7 << \" \"<< 9999999 % 7 << \" \"<< 99999999 % 7 << \" \"<< 999999999 % 7 << \" \"<< 9999999999 % 7 << \" \"<< 99999999999 % 7 << \" \"<<endl;\n        // cout << 10 % 7 << \" \" << (100) % 7 << \" \"<< (1000) % 7 << \" \"<< (10000) % 7 << \" \"<< (100000) % 7 << \" \"<< (1000000) % 7 << \" \"<< (10000000) % 7 << \" \"<< (100000000) % 7 << \" \"<< (1000000000) % 7 << \" \"<< (10000000000) % 7 << \" \"<< (100000000000) % 7 << \" \"<<endl;\n        if (k == 1 || k == 3 || k == 9) {\n            return string(n, '9');\n        }\n        if (k == 2) {\n            if (n < 2) return string(n, '8');\n            return \"8\" + string(n - 2, '9') + \"8\";\n        }\n        if (k == 4) {\n            if (n < 5) return string(n, '8');\n            return \"88\" + string(n - 4, '9') + \"88\";\n        }\n        if (k == 5) {\n            if (n < 2) return string(n, '5');\n            return \"5\" + string(n - 2, '9') + \"5\";\n        }\n        if (k == 6) {\n            if (n <= 2) return string(n, '6');\n            if (n % 2 == 1) return \"8\" + string((n - 3) / 2 , '9') + \"8\" + string((n - 3) / 2 , '9') + \"8\";\n            return \"8\" + string((n - 4) / 2 , '9') + \"77\" + string((n - 4) / 2 , '9') + \"8\";\n        }\n        if (k == 7) {\n            if (n <= 2) return string(n, '7');\n            if (n % 2 == 1) {\n                return string(n / 2, '9') + f(1, n / 2) + string(n / 2, '9') ;\n            } else {\n                return string(n / 2 - 1, '9') + f(2, n / 2 - 1) + string(n / 2 - 1, '9') ;\n            }\n        }\n        if (k == 8) {\n            if (n < 7) return string(n, '8');\n            return \"888\" + string(n - 6, '9') + \"888\";\n        }\n        return \"\";\n    }\n};",
