    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if k in [1, 3, 9]:\n            return \"9\" * n\n        if k == 2:\n            if n <= 2:\n                return \"8\" * n\n            return \"8\" + \"9\" * (n - 2) + \"8\"\n        if k == 4:\n            if n <= 4:\n                return \"8\" * n\n            return \"88\" + \"9\" * (n - 4) + \"88\"\n        if k == 5:\n            if n <= 2:\n                return \"5\" * n\n            return \"5\" + \"9\" * (n - 2) + \"5\"\n        if k == 8:\n            if n <= 6:\n                return \"8\" * n\n            return \"888\" + \"9\" * (n - 6) + \"888\"\n        if k == 6:\n            if n <= 2:\n                return \"6\" * n\n            if n & 1 == 1:\n                v = (n - 3) >> 1\n                return \"8\" + \"9\" * v + \"8\" + \"9\" * v + \"8\"\n            v = (n - 4) >> 1\n            return \"8\" + \"9\" * v + \"77\" + \"9\" * v + \"8\"\n        # k == 7:\n        p = n // 12\n        q = n % 12\n        if q == 0 or q == 6:\n            return \"9\" * n\n        if q == 1:\n            return \"999999\" * p + \"7\" + \"999999\" * p\n        if q == 2:\n            return \"999999\" * p + \"77\" + \"999999\" * p\n        if q == 3:\n            return \"999999\" * p + \"959\" + \"999999\" * p\n        if q == 4:\n            return \"999999\" * p + \"9779\" + \"999999\" * p\n        if q == 5:\n            return \"999999\" * p + \"99799\" + \"999999\" * p\n        if q == 7:\n            return \"999999\" * p + \"9994999\" + \"999999\" * p\n        if q == 8:\n            return \"999999\" * p + \"99944999\" + \"999999\" * p\n        if q == 9:\n            return \"999999\" * p + \"999969999\" + \"999999\" * p\n        if q == 10:\n            return \"999999\" * p + \"9999449999\" + \"999999\" * p\n        # q == 11:\n        return \"999999\" * p + \"99999499999\" + \"999999\" * p\n",
