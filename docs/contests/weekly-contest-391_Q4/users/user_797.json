    "submission": "class Solution {\npublic:\n    \n    static bool cmp1(vector<int> &a, vector<int> &b){\n        return a[0]+a[1]<b[0]+b[1];\n    }\n    \n    static bool cmp2(vector<int> &a, vector<int> &b){\n        return a[0]-a[1]<b[0]-b[1];\n    }\n    \n    int minimumDistance(vector<vector<int>>& a) {\n        int n = a.size();\n        map<pair<int,int>, int> m;\n        for(auto &i : a)m[{i[0],i[1]}]++;\n        auto b = a;\n        sort(a.begin(), a.end(), cmp1);\n        sort(b.begin(), b.end(), cmp2);\n        int ans = INT_MAX;\n        if(b[0][0]==a[0][0] && b[0][1]==a[0][1])\n            ans = min(ans, max(abs(a[1][0]-a[n-1][0])+abs(a[1][1]-a[n-1][1]), abs(b[1][0]-b[n-1][0])+abs(b[1][1]-b[n-1][1])));\n        else if(b[n-1][0]==a[0][0] && b[n-1][1]==a[0][1])\n            ans = min(ans, max(abs(a[1][0]-a[n-1][0])+abs(a[1][1]-a[n-1][1]), abs(b[0][0]-b[n-2][0])+abs(b[0][1]-b[n-2][1])));\n        else\n            ans = min(ans, max(abs(a[1][0]-a[n-1][0])+abs(a[1][1]-a[n-1][1]), abs(b[0][0]-b[n-1][0])+abs(b[0][1]-b[n-1][1])));\n        if(b[0][0]==a[n-1][0] && b[0][1]==a[n-1][1])\n            ans = min(ans, max(abs(a[0][0]-a[n-2][0])+abs(a[0][1]-a[n-2][1]), abs(b[1][0]-b[n-1][0])+abs(b[1][1]-b[n-1][1])));\n        else if(b[n-1][0]==a[n-1][0] && b[n-1][1]==a[n-1][1])\n            ans = min(ans, max(abs(a[0][0]-a[n-2][0])+abs(a[0][1]-a[n-2][1]), abs(b[0][0]-b[n-2][0])+abs(b[0][1]-b[n-2][1])));\n        else\n            ans = min(ans, max(abs(a[0][0]-a[n-2][0])+abs(a[0][1]-a[n-2][1]), abs(b[0][0]-b[n-1][0])+abs(b[0][1]-b[n-1][1])));\n        if(a[0][0]==b[0][0] && a[0][1]==b[0][1])\n            ans = min(ans, max(abs(a[1][0]-a[n-1][0])+abs(a[1][1]-a[n-1][1]), abs(b[1][0]-b[n-1][0])+abs(b[1][1]-b[n-1][1])));\n        else if(a[n-1][0]==b[0][0] && a[n-1][1]==b[0][1])\n            ans = min(ans, max(abs(a[0][0]-a[n-2][0])+abs(a[0][1]-a[n-2][1]), abs(b[1][0]-b[n-1][0])+abs(b[1][1]-b[n-1][1])));\n        else\n            ans = min(ans, max(abs(a[0][0]-a[n-1][0])+abs(a[0][1]-a[n-1][1]), abs(b[1][0]-b[n-1][0])+abs(b[1][1]-b[n-1][1])));\n        if(a[0][0]==b[n-1][0] && a[0][1]==b[n-1][1])\n            ans = min(ans, max(abs(a[1][0]-a[n-1][0])+abs(a[1][1]-a[n-1][1]), abs(b[0][0]-b[n-2][0])+abs(b[0][1]-b[n-2][1])));\n        else if(a[n-1][0]==b[n-1][0] && a[n-1][1]==b[n-1][1])\n            ans = min(ans, max(abs(a[0][0]-a[n-2][0])+abs(a[0][1]-a[n-2][1]), abs(b[0][0]-b[n-2][0])+abs(b[0][1]-b[n-2][1])));\n        else\n            ans = min(ans, max(abs(a[0][0]-a[n-1][0])+abs(a[0][1]-a[n-1][1]), abs(b[0][0]-b[n-2][0])+abs(b[0][1]-b[n-2][1])));\n        return ans;\n    }\n};"
