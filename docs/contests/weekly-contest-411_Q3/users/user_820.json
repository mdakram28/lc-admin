    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if k == 1 or k== 3 or k== 9:\n            return ''.join(['9']*n)\n        if k == 2:\n            if n == 1:\n                return '8'\n            elif n>=1:\n                return '8'+ ''.join(['9']*(n-2)) +'8'\n        if k == 4:\n            if n == 1:\n                return '8'\n            elif n<=4:\n                return ''.join(['8']*(n))\n            else :\n                return '88'+ ''.join(['9']*(n-4)) +'88'\n        if k == 5:\n            if n==1:\n                return '5'\n            else :\n                return '5'+ ''.join(['9']*(n-2)) +'5'\n        if k == 6:\n            if n==1:\n                return '6'\n            elif n==2:\n                return '66'\n            elif n==4:\n                return '8778'\n            elif n%2 == 0:\n                return '8'+ ''.join(['9']*((n-3)//2)) +'77' +''.join(['8']*((n%2))) + ''.join(['9']*((n-3)//2))+'8'\n            else:\n                return '8'+ ''.join(['9']*((n-2)//2)) + ''.join(['8']*((n%2))) + ''.join(['9']*((n-2)//2))+'8'\n        if k == 7:\n            if n==1:\n                return '7'\n            elif n==2:\n                return '77'\n            else:\n                a = ['5', '7', '4', '6', '4', '7']\n                b = ['77','99', '44', '44', '99', '77']\n                if n%2==0:\n                    k = ((n//2)-2)%6\n                    return ''.join(['9']*((n-1)//2)) + b[k] +''.join(['9']*((n-1)//2))\n                else:\n                    k = ((n//2)-1)%6\n                    return ''.join(['9']*((n)//2)) + a[k] +''.join(['9']*((n)//2))                    \n        if k==8:\n            if n == 1:\n                return '8'\n            elif n<=6:\n                return ''.join(['8']*(n))\n            else :\n                return '888'+ ''.join(['9']*(n-6)) +'888'\n\n        return -1",
