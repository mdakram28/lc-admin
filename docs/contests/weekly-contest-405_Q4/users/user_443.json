\u280b    \u2809\u28b9\u28ff\u28ff\u28ff\u28ff\u2847        \u2808\u283b\u28ff\u28ff\u28ff\u28e7\u28bb\u28ff\u28ff\u28ff\u28ff\n   \u2819\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u283f\u281f\u283b\u283f\u283f\u2817    \u2819\u2822   \u2818\u281b\u283f\u283f\u283f\u281f\u281b\u281b\u2813 \u2818\u2801     \u281a\u283f\u283f\u283f\u283f\u283f\u2837          \u2808\u283b\u283f\u283f\u280e\u283f\u283f\u283f\u283f\n   \n   \u2800\u2800\u2800\u2800\u2800\u2800\u2800\u2800\n   */\n   \n   \n   \n\nstruct TrieNode {\n    unordered_map<char, TrieNode*> children;\n    vector<pair<int, int>> word_indices_and_costs;\n};\n\nclass Trie {\npublic:\n    TrieNode* root;\n    Trie() {\n        root = new TrieNode();\n    }\n    \n    void insert(const string& word, int index, int cost) {\n        TrieNode* node = root;\n        for (char ch : word) {\n            if (!node->children.count(ch)) {\n                node->children[ch] = new TrieNode();\n            }\n            node = node->children[ch];\n        }\n        node->word_indices_and_costs.push_back({index, cost});\n    }\n};\n\nclass Solution {\npublic:\n    int minimumCost(string target, vector<string>& words, vector<int>& costs) {\n        int n = target.length();\n        vector<int> dp(n + 1, INT_MAX);\n        dp[0] = 0;\n        \n        Trie trie;\n        for (int i = 0; i < words.size(); ++i) {\n            trie.insert(words[i], i, costs[i]);\n        }\n        \n        for (int i = 0; i < n; ++i) {\n            if (dp[i] == INT_MAX) continue;\n            \n            TrieNode* node = trie.root;\n            for (int j = i; j < n && node; ++j) {\n                if (!node->children.count(target[j])) break;\n                node = node->children[target[j]];\n                \n                for (const auto& [index, cost] : node->word_indices_and_costs) {\n                    int len = words[index].length();\n                    if (i + len <= n) {\n                        dp[i + len] = min(dp[i + len], dp[i] + cost);\n                    }\n                }\n            }\n        }\n        \n        return dp[n] == INT_MAX ? -1 : dp[n];\n    }\n};",
