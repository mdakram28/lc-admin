    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if k == 1 or k == 3 or k == 9:\n            return \"9\"*n\n        if k == 2:\n            if n == 1:\n                return \"8\"\n            return \"8\" + \"9\"*(n-2) + \"8\"\n        if k == 4:\n            if n == 1:\n                return \"8\"\n            elif n == 2:\n                return \"88\"\n            elif n == 3:\n                return \"888\"\n            elif n == 4:\n                return \"8888\"\n            return \"88\" + \"9\"*(n-4) + \"88\"\n        if k == 8:\n            if n <= 6:\n                return \"8\"*n\n            return \"8\"*3 + \"9\"*(n-6)+\"8\"*3\n        if k == 5:\n            if n == 1:\n                return \"5\"\n            return \"5\" + \"9\"*(n-2) + \"5\"\n        if k == 6:\n            if n == 1:\n                return \"6\"\n            elif n == 2:\n                return \"66\"\n            if n % 2 == 0:\n                return \"8\" + \"9\"*(n//2 - 2) + \"77\" + \"9\"*(n//2 - 2) + \"8\"\n            else:\n                return \"8\" + \"9\"*((n-1)//2 - 1) + \"8\" + \"9\"*((n-1)//2 - 1) + \"8\"\n        # otherwise, k == 7\n        if n == 1:\n            return \"7\"\n        if n == 2:\n            return \"77\"\n        if n == 3:\n            return \"959\"\n        if n == 4:\n            return \"9779\"\n        if n == 5:\n            return \"99799\"\n        \n        if n %6 == 0:\n            return \"9\"*n\n        \n        if (n-6) % 12 <= 5:\n            # first group\n            if (n-6) % 12 == 1:\n                return \"9\"*(n//2) + \"4\" + \"9\"*(n//2)\n            elif (n-6) % 12 == 2:\n                return \"9\"*(n//2 - 1) + \"44\" + \"9\"*(n//2 - 1)\n            elif (n-6) % 12 == 3:\n                return \"9\"*(n//2) + \"6\" + \"9\"*(n//2)\n            elif (n-6) % 12 == 4:\n                return \"9\"*(n//2 - 1) + \"44\" + \"9\"*(n//2 - 1)\n            else:\n                return \"9\"*(n//2) + \"4\" + \"9\"*(n//2)\n        else:\n            # second group\n            if (n-6) % 12 == 7:\n                return \"9\"*(n//2) + \"7\" + \"9\"*(n//2)\n            elif (n-6) % 12 == 8:\n                return \"9\"*(n//2 - 1) + \"77\" + \"9\"*(n//2 - 1)\n            elif (n-6) % 12 == 9:\n                return \"9\"*(n//2) + \"5\" + \"9\"*(n//2)\n            elif (n-6) % 12 == 10:\n                return \"9\"*(n//2 - 1) + \"77\" + \"9\"*(n//2 - 1)\n            else:\n                return \"9\"*(n//2) + \"7\" + \"9\"*(n//2)\n        return \"\"",
