    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        # 1,2,3,4,5,8,9,6,\n        # 7\n        if k == 1 or k == 3:\n            return \"9\"*n\n        if k == 2:\n            return \"8\" + \"9\"*(n-2) + \"8\"*(n > 1)\n        if k == 4:\n            if n >= 5:\n                return \"8\"*2+\"9\"*(n-4)+\"8\"*2\n            return \"8\"*n \n        if k == 5:\n            return \"5\"+ \"9\"*(n-2) +\"5\" * (n > 1)\n        if k == 6:\n            if n == 1:\n                return \"6\"\n            if n == 2:\n                return \"66\"\n            if n == 3:\n                return \"888\"\n            if n == 4:\n                return \"8778\"\n            if n == 5:\n                return \"89898\"\n            if n % 2 == 0:\n                return \"8\" + \"9\" * ((n-4)//2) + \"77\" + \"9\" * ((n-4)//2) + \"8\"\n            else:\n                return \"8\" + \"9\" * ((n-3)//2) + \"8\" + \"9\" * ((n-3)//2) + \"8\"\n        if k == 7:\n            if n == 1:\n                return \"7\"\n            if n == 2:\n                return \"77\"\n            if n == 3:\n                return \"959\"\n            if n == 4:\n                return \"9779\"\n            if n == 5:\n                return \"99799\"\n            if n == 6:\n                return \"999999\"\n            if n == 7:\n                return \"9994999\"\n            if n == 8:\n                return \"99944999\"\n            if n == 9:\n                return \"999969999\"\n            if n == 10:\n                return \"9999449999\"\n            if n == 11:\n                return \"99999499999\"\n            if n == 12:\n                return \"999999999999\"\n            if n == 13:\n                return \"9999997999999\"\n            if n == 14:\n                return \"99999977999999\"\n            if n == 15:\n                return \"999999959999999\"\n            if n == 16:\n                return \"9999999779999999\"\n            if n == 17:\n                return \"99999999799999999\"\n            if n == 18:\n                return \"999999999999999999\"\n            MOD = n % 12\n            if MOD % 6 == 0:\n                return \"9\"*n\n            if MOD in [1,2,4,5]:\n                MIDDLES = {1:1,2:2,4:2,5:1}[MOD]\n                return \"9\" * ((n-MIDDLES)//2) + \"7\" * MIDDLES + \"9\" * ((n-MIDDLES)//2)\n            if MOD == 3:\n                return \"9\" * ((n-1)//2) + \"5\" + \"9\" * ((n-1)//2)\n            if MOD in [7,8,10,11]:\n                MIDDLES = {7:1,8:2,10:2,11:1}[MOD]\n                return \"9\" * ((n-MIDDLES)//2) + \"4\" * MIDDLES + \"9\" * ((n-MIDDLES)//2)\n            if MOD == 9:\n                return \"9\" * ((n-1)//2) + \"6\" + \"9\" * ((n-1)//2)\n        if k == 8:\n            if n < 7:\n                return \"8\"*n\n            if n >= 7:\n                ret = 3*\"8\" + (n-6)*\"9\" + 3*\"8\"\n            return ret\n        if k == 9:\n            return \"9\"*n",
