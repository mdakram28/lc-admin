    "submission": "class Solution {\npublic:\n    string largestPalindrome(int n, int k) {\n        if(n==1){\n            for(int i=9;i>=1;i--){\n                if(i%k==0){return to_string(i);}\n            }\n        }\n        if(n==2){\n            for(int i=99;i>=11;i--){\n                if(i%k==0 and i/10==i%10){return to_string(i);}\n            }\n        }\n        string s;\n        int x = n;\n        while(x--)s.push_back('9');\n        if(k==1 or k==3 or k==9){\n            return s;\n        }\n        if(k==2){\n            s[0]='8'; s[n-1]='8';return s;\n        }\n        if(k==5){\n            s[0]='5'; s[n-1]='5';return s;\n        }\n        if(k==4){\n            s[0]='8'; s[n-1]='8';s[1]='8'; s[n-2]='8';return s;\n        }\n        if(k==6){\n            s[0]='8'; s[n-1]='8';\n            if(n%2)s[n/2]='8'; \n            else{s[n/2]='7';s[n/2-1]='7';}\n            return s;\n        }\n        if(k==8){\n            s[0]='8'; s[n-1]='8';s[1]='8'; s[n-2]='8';\n            s[2]='8';s[n-3]='8';\n            return s;\n        }\n        if(k==7){\n            if(n%12==1){s[n/2]='7';}\n            if(n%12==2){s[n/2]='7';s[n/2-1]='7';}\n            if(n%12==3){s[n/2]='5';}\n            if(n%12==4){s[n/2]='7';s[n/2-1]='7';}\n            if(n%12==5){s[n/2]='7';}\n            if(n%12==7){s[n/2]='4';}\n            if(n%12==8){s[n/2]='4'; s[n/2-1]='4';}\n            if(n%12==9){s[n/2]='6';}\n            if(n%12==10){s[n/2]='4';s[n/2-1]='4';}\n            if(n%12==11){s[n/2]='4';}\n        }\n            return s;\n    }\n};",
