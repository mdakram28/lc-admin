    "submission": "class Solution {\npublic:\n    string largestPalindrome(int n, int k) {\n        string ans(n, ' ');\n        if (k == 1 || k == 3 || k == 9) {\n            for (int i = 0; i < n; i++)\n                ans[i] = '9';\n        }\n        if (k == 2) {\n            ans[0] = '8'; ans[n - 1] = '8';\n            for (int i = 1; i < n - 1; i++)\n                ans[i] = '9'; \n        }\n        if (k == 4) {\n            for (int i = 0; i < 2; i++) {\n                if (i < n)\n                    ans[i] = '8';\n                if (n - 1 - i >= 0)\n                    ans[n - 1 - i] = '8';\n            }\n            for (int i = 2; i < n - 2; i++)\n                ans[i] = '9';\n        }\n        if (k == 8) {\n            for (int i = 0; i < 3; i++) {\n                if (i < n)\n                    ans[i] = '8';\n                if (n - 1 - i >= 0)\n                    ans[n - 1 - i] = '8';\n            }\n            for (int i = 3; i < n - 3; i++)\n                ans[i] = '9';\n        }\n        if (k == 5) {\n            ans[0] = '5'; ans[n - 1] = '5';\n            for (int i = 1; i < n - 1; i++)\n                ans[i] = '9';\n        }\n        if (k == 6) {\n            if (n <= 2) {\n                ans[0] = '6'; ans[n - 1] = '6';\n            } else {\n                ans[0] = '8'; ans[n - 1] = '8';\n                if (n % 2 == 1) {\n                    for (int i = 1; i < n / 2; i++) {\n                        ans[i] = '9';\n                        ans[n - 1 - i] = '9'; \n                    }\n                    ans[n / 2] = '8';\n                } else {\n                    for (int i = 1; i < n / 2 - 1; i++) {\n                        ans[i] = '9';\n                        ans[n - 1 - i] = '9';\n                    }\n                    ans[n / 2 - 1] = '7';\n                    ans[n / 2] = '7';\n                }\n            }\n        }\n        if (k == 7) {\n            int tmp0 = n / 12, tmp1 = n % 12;\n            for (int i = 0; i < tmp0; i++) {\n                for (int j = 0; j < 6; j++) {\n                    ans[i * 6 + j] = '9';\n                    ans[n - 1 - i * 6 - j] = '9';\n                }\n            }\n            string tmp2[11] = {\"7\", \"77\", \"959\", \"9779\", \"99799\", \"999999\", \"9994999\", \"99944999\", \"999969999\", \"9999449999\", \"99999499999\"};\n            for (int i = 0; i < tmp1; i++)\n                ans[6 * tmp0 + i] = tmp2[tmp1 - 1][i];\n        }\n        return ans;\n    }\n};",
