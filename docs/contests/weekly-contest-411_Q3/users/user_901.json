    "submission": "\nclass Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n\n        res = \"0\"\n\n        if k == 1:\n            res = \"9\" * (n)\n        if k == 2:\n            if n == 1: res = \"8\"\n            else: res = \"8\" + \"9\" * (n-2) + \"8\"\n\n        if k == 3:\n            res = \"9\" * (n)\n\n        if k == 4:\n            if n <= 4:\n                res = \"8\" * n\n            else:\n                res = \"88\" + \"9\" * (n-4) + \"88\"\n\n            # if n == 1: res = \"8\"\n            # else: res = \"6\" + \"9\" * (n-2) + \"6\"\n\n        if k == 5:\n            if n == 1: res = \"5\"\n            else:\n                res = \"5\" + \"9\" * (n-2) + \"5\"\n\n\n        if k == 6:\n            if n == 1: res = \"6\"\n            elif n == 2: res = \"66\"\n            else:\n                if n % 2 == 0:\n                    res = \"8\" + \"9\" * (n//2-2) + \"77\" + \"9\" * (n//2-2) + \"8\"\n                else:\n                    res = \"8\" + \"9\" * (n//2-1) + \"8\" + \"9\" * (n//2-1) + \"8\"\n\n\n        if k == 7:\n\n            idx = n % 12\n            stk = [\"99\", \"7\", \"77\", \"5\", \"77\", \"7\", \"99\", \"4\", \"44\", \"6\", \"44\",\"4\"]\n\n            res = \"9\" * ((n+1)//2-1) + stk[idx] + \"9\" * ((n+1)//2-1)\n\n\n        if k == 8:\n            if n <= 6: res = \"8\" * n\n            else:\n                res = \"888\" + \"9\" * (n-6) + \"888\"\n\n        if k == 9:\n            res = \"9\" * (n)\n\n\n        return res",
