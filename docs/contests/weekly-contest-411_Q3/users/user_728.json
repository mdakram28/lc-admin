    "submission": "class Solution {\npublic:\n    string largestPalindrome(int n, int k) {\n        string s(n,'*');\n        if(k==1){\n            for(int i=0;i<n;i++){\n                s[i]='9';\n            }\n        }\n        else if(k==2){\n            if(n==1){\n                s=\"8\";\n            }\n            else{\n            s[0]='8';\n            s[n-1]='8';\n            for(int i=1;i<n-1;i++){\n                s[i]='9';\n                s[n-i-1]='9';\n            }\n            }\n        }\n        else if(k==5){\n            if(n==1){\n                s=\"5\";\n            }\n            else{\n            s[0]='5';\n            s[n-1]='5';\n            for(int i=1;i<n-1;i++){\n                s[i]='9';\n            }\n            }\n        }\n        else if(k==3){\n            for(int i=0;i<n;i++){\n                s[i]='9';\n            }\n        }\n        else if(k==9){\n            for(int i=0;i<n;i++){\n                s[i]='9';\n            }\n        }\n        else if(k==6){\n            if(n==1){\n                s=\"6\";\n            }\n            else if(n==2){\n                s[0]='6';\n                s[1]='6';\n            }\n            else{\n                s[0]='8';\n                s[n-1]='8';\n\n                int sm=16;\n                for(int i=1;i<n/2;i++){\n                    sm+=9;\n                    s[i]='9';\n                    s[n-i-1]='9';\n                }\n                s[n/2]='8';\n                if(n%2==0){\n                    s[n/2]='7';\n                    s[n-1-n/2]='7';\n                }\n            }\n        }\n        else if(k==4){\n            if(n==1)return \"8\";\n            if(n==2)return \"88\";\n            s[n-1]='8';\n            s[0]='8';\n            s[n-2]='8';\n            s[1]='8';\n            for(int i=2;i<n-2;i++){\n                s[i]='9';\n            }\n        }\n        else if(k==8){\n            if(n==1)return \"8\";\n            if(n==2)return \"88\";\n            s[0]='8';\n            s[n-1]='8';\n            s[1]='8';\n            s[n-2]='8';\n            s[2]='8';\n            s[n-3]='8';\n            for(int i=3;i<n-3;i++){\n                s[i]='9';\n            }\n        }\n        else if(k==7){\n            if(n==1){\n                s=\"7\";\n            }\n            else if(n==2){\n                s=\"77\";\n            }\n            else{\n                if(n%2){\n                    for(int i=0;i<n/2;i++){\n                        s[i]='9';\n                        s[n-i-1]='9';\n                    }\n                    for(int i=9;i>=0;i--){\n                        string str=s;\n                        str[n/2]='0'+i;\n                        int sm=0;\n                        for(int j=0;j<n;j++){\n                            sm=(sm*10+(str[j]-'0'))%7;\n                        }\n                        // cout << sm <<endl;\n                        if(sm%7==0){\n                            s=str;break;\n                        }\n                    }\n                }\n                else{\n                    for(int i=0;i<n/2-1;i++){\n                        s[i]='9';\n                        s[n-i-1]='9';\n                    }\n                    for(int i=9;i>=0;i--){\n                        string str=s;\n                        str[n/2]='0'+i;\n                        str[n-1-n/2]='0'+i;\n                        int sm=0;\n                        for(int j=0;j<n;j++){\n                            sm=(sm*10+(str[j]-'0'))%7;\n                        }\n                        if(sm%7==0){\n                            s=str;break;\n                        }\n                    }\n                }\n            }\n        }\n        return s;\n    }\n};",
