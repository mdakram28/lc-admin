    "submission": "class Solution {\npublic:\n    int minFlips(vector<vector<int>>& a) {\n        int m=a.size(), n= a[0].size();\n        int c=0;\n        for(int i=0; i<m/2; i++)\n        {\n            for(int j=0; j<n/2; j++)\n            {\n                int x=0, y=0;\n                if(a[i][j]==1) x++;\n                else y++;\n                if(a[m-i-1][j]==1) x++;\n                else y++;\n                if(a[i][n-j-1]==1) x++;\n                else y++;\n                if(a[m-i-1][n-j-1]==1) x++;\n                else y++;\n                c+=min(4-x, 4-y);\n            }\n        }\n        int x=0;\n        int c1=0, c2=0;\n        if(m%2 and n%2)\n        {\n            for(int i=0; i<m/2; i++)\n            {\n                if(a[i][n/2]!= a[m-i-1][n/2])\n                    c1++;\n                else if(a[i][n/2]==1) x+=2;\n            }\n            for(int i=0; i<n/2; i++)\n            {\n                if(a[m/2][i]!= a[m/2][n-i-1]) c2++;\n                else if(a[m/2][i]==1) x+=2;\n            }\n            if(x%4==0){\n                c+=c1+c2+a[m/2][n/2];\n            }\n            else{\n                if(c1>0 or c2>0){\n                    c+=c1+c2+a[m/2][n/2];\n                }\n                else{\n                    c+=2+a[m/2][n/2];\n                }\n            }\n        }\n        else if(m%2)\n        {\n            for(int i=0; i<n/2; i++)\n            {\n                if(a[m/2][i]!= a[m/2][n-i-1]) c2++;\n                else if(a[m/2][i]==1) x+=2;\n            }\n            if(x%4==0)\n            {\n                c+=c2;\n            }\n            else{\n                if(c2>0) c+=c2;\n                else c+=2;\n            }\n        }\n        else if(n%2)\n        {\n            for(int i=0; i<m/2; i++)\n            {\n                if(a[i][n/2]!= a[m-i-1][n/2])\n                    c1++;\n                else if(a[i][n/2]==1) x+=2;\n            }\n            if(x%4==0)\n            {\n             