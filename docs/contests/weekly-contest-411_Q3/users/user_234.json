    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if k == 1 or k == 3 or k == 9:\n            return '9'*n\n        if k == 2:\n            if n == 1:\n                return '8'\n            return '8'+'9'*(n-2)+'8'\n        if k == 4:\n            if n < 4:\n                return '8'*n\n            return '88'+'9'*(n-4)+'88'\n        if k == 5:\n            if n == 1:\n                return '5'\n            return '5'+'9'*(n-2)+'5'\n        if k == 6:\n            if n < 3:\n                return '6'*n\n            if n%2:\n                r = ((pow(10,n//2,6)-1)*(1+pow(10,n//2+1,6))-1-pow(10,n-1,6))%6\n                for d in reversed(range(10)):\n                    if (r+d*pow(10,n//2,6))%6 == 0:\n                        return '8'+'9'*(n//2-1)+str(d)+'9'*(n//2-1)+'8'\n            else:\n                r = ((pow(10,n//2-1,6)-1)*(1+pow(10,n//2+1,6))-1-pow(10,n-1,6))%6\n                for d in reversed(range(10)):\n                    if (r+d*11*pow(10,n//2-1,6))%6 == 0:\n                        return '8'+'9'*(n//2-2)+str(d)*2+'9'*(n//2-2)+'8'\n        if k == 7:\n            if n < 3:\n                return '7'*n\n            if n%2:\n                r = ((pow(10,n//2,7)-1)*(1+pow(10,n//2+1,7)))%7\n                for d in reversed(range(10)):\n                    if (r+d*pow(10,n//2,7))%7 == 0:\n                        return '9'*(n//2)+str(d)+'9'*(n//2)\n            else:\n                r = ((pow(10,n//2-1,7)-1)*(1+pow(10,n//2+1,7)))%7\n                for d in reversed(range(10)):\n                    if (r+d*11*pow(10,n//2-1,7))%7 == 0:\n                        return '9'*(n//2-1)+str(d)*2+'9'*(n//2-1)\n        if k == 8:\n            if n < 6:\n                return '8'*n\n            return '888'+'9'*(n-6)+'888'",
