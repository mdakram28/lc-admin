    "submission": "class Solution:\n    def maxOperations(self, s: str) -> int:\n        # t = 3\n        # 1 1 1 2 3 4 4 5\n        # 100011001010111001\n        # 000111001010111001\n        # 000110011010111001\n        # 000100111010111001\n        # 000001111010111001\n        # 000001110110111001\n        # 000001101110111001\n        # 000001011110111001\n        # 000000111110111001\n        # 000000111101111001\n        # 000000111011111001\n        # 000000110111111001\n        # 000000101111111001\n        # 000000011111111001\n        # 000000011111110011\n        # 000000011111100111\n        # 000000011111001111\n        # 000000011110011111\n        # 000000011100111111\n        # 000000011001111111\n        # 000000010011111111\n        # 000000000111111111\n\n        import re\n\n\n        g = re.split('0+', s)\n        g = [group.strip(\"0\") for group in g if group]\n        \n        # print(g)\n        ans = 0\n        if s[-1] == \"0\":\n            c = 1\n            for i in range(len(g) - 1, -1, -1):\n                ans += c * len(g[i])\n                c += 1\n\n            return ans\n        else:\n            c = 0\n            for i in range(len(g) - 1, -1, -1):\n                ans += c * len(g[i])\n                c += 1\n\n            return ans\n        return ans\n    ",
