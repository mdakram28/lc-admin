    "submission": "class Solution:\n    def largestPalindrome(self, n: int, k: int) -> str:\n        if k == 1 or k == 3 or k == 9:\n            return '9' * n\n        if k == 2:\n            if n == 1:\n                return '8'\n            elif n == 2:\n                return '88'\n            else:\n                return '8' + '9' * (n - 2) + '8'\n        if k == 4:\n            if n <= 3:\n                return '8' * n\n            else:\n                return '88' + '9' * (n - 4) + '88'\n        if k == 5:\n            if n <= 2:\n                return '5' * n\n            else:\n                return '5' + '9' * (n - 2) + '5'\n        if k == 6:\n            if n == 1:\n                return '6'\n            if n == 2:\n                return '66'\n            else:\n                if n % 2 != 0:\n                    return '8' + ((n - 3) // 2) * '9' + '8' + ((n - 3) // 2) * '9' + '8'\n                else:\n                    return '8' + ((n - 4) // 2) * '9' + '77' + ((n - 4) // 2) * '9' + '8'\n        if k == 7:\n            l = ['7', '77', '959', '9779', '99799', '999999', '9994999', '99944999', '999969999', '9999449999', '99999499999', '999999999999']\n            if n <= 12:\n                return l[n - 1]\n            else:\n                if n%12==0:\n                    return '9'*n\n                else:\n                    half = (n - len(l[(n % 12) -1])) // 2\n                    return '9' * half + l[(n % 12) -1] + '9' * half\n        if k == 8:\n            if n <= 6:\n                return '8' * n\n            else:\n                return '888' + '9' * (n - 6) + '888'\n",
