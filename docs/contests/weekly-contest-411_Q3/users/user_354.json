    "submission": "class Solution {\npublic:\n    string largestPalindrome(int n, int k) {\n        string s;\n        if(k==1){\n            for(int i=0;i<n;i++){s+=\"9\";}\n        }\n        else if(k==2){\n            if(n==1){s=\"8\";}\n            else if(n==2){s=\"88\";}\n            else{\n                s+=\"8\";\n                for(int i=1;i<(n-1);i++){s+=\"9\";}\n                s+=\"8\";\n            }\n        }\n        else if(k==3){\n            for(int i=0;i<n;i++){s+=\"9\";}\n        }\n        else if(k==4){\n            if(n==1){s=\"8\";}\n            else if(n==2){s=\"88\";}\n            else if(n==3){s=\"888\";}\n            else{\n                s+=\"88\";\n                for(int i=0;i<(n-4);i++){\n                    s+=\"9\";\n                }\n                s+=\"88\";\n            }\n        }\n        else if(k==5){\n            if(n==1){s=\"5\";}\n            else if(n==2){s=\"55\";}\n            else{\n                s+=\"5\";\n                for(int i=1;i<(n-1);i++){s+=\"9\";}\n                s+=\"5\";\n            }\n        }\n        else if(k==6){\n            if(n==1){s=\"6\";}\n            else if(n==2){s=\"66\";}\n            else if(n==3){s=\"888\";}\n            else{\n                s+=\"8\";\n                if(n%2==0){\n                    for(int i=0;i<((n/2)-2);i++){\n                        s+=\"9\";\n                    }\n                    s+=\"77\";\n                    for(int i=0;i<((n/2)-2);i++){\n                        s+=\"9\";\n                    }                    \n                }\n                else{\n                    for(int i=0;i<((n/2)-1);i++){\n                        s+=\"9\";\n                    }\n                    s+=\"8\";\n                    for(int i=0;i<((n/2)-1);i++){\n                        s+=\"9\";\n                    }                      \n                }\n                s+=\"8\";\n            }\n        }\n        else if(k==7){\n            if(n==1){s=\"7\";}\n            else if(n==2){s=\"77\";}\n            else if(n==3){s=\"959\";}\n            else{\n                string str[12]={\n                    \"7\",\"77\",\"5\",\"77\",\"7\",\"\",\"4\",\"44\",\"6\",\"44\",\"4\",\"\"\n                };\n                int ind=(n%12+11)%12;\n                for(int i=0;i<((n-str[ind].size())/2);i++){\n                    s+=\"9\";\n                }\n                s+=str[ind];\n                for(int i=0;i<((n-str[ind].size())/2);i++){\n                    s+=\"9\";\n                }                \n            }\n        }\n        else if(k==8){\n            if(n==1){s=\"8\";}\n            else if(n==2){s=\"88\";}\n            else if(n==3){s=\"888\";}\n            else if(n==4){s=\"8888\";}\n            else if(n==5){s=\"88888\";}\n            else{\n                s+=\"888\";\n                for(int i=0;i<(n-6);i++){\n                    s+=\"9\";\n                }\n                s+=\"888\";\n            }\n        }\n        else{\n            for(int i=0;i<n;i++){s+=\"9\";}\n        }\n        return s;\n    }\n};",
